<Window x:Class="IHolographyH1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IHolographyH1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        WindowState="Maximized">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="37" Margin="81,82,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="55"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="316" Margin="229,70,0,0" VerticalAlignment="Top" Width="457"/>
        <Button x:Name="button" Content="ResAlm" HorizontalAlignment="Left" Height="95" Margin="50,124,0,0" VerticalAlignment="Top" Width="124" Click="button_Click"/>
        <Button x:Name="button1" Content="SetBeep" HorizontalAlignment="Left" Margin="81,295,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <Button x:Name="button2" Content="ListRead" HorizontalAlignment="Left" Margin="81,28,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>
        <Button x:Name="button3" Content="DataGrid" HorizontalAlignment="Left" Margin="50,245,0,0" VerticalAlignment="Top" Width="75" Click="button3_Click"/>

    </Grid>
</Window>
